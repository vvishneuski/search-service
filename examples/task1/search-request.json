        "query": "v2/skill-link/search?q=objectIds.id=in=({{parents}})&fields=objectIds.id,spokenLanguages.skillName",
        "parentPath": "employee.global_user_id",
        "collectionName": "spoken_languages"
      }
    ]
  },
  "mapping": "[0].spoken_languages"
}
