{
  "request": {
    "primaryKey": "global_user_id",
    "query": "v2/epampersonpublic/search?q=business_email==${advisor:email}",
    "parentPath": "",
    "collectionName": "employee",
    "descendants": [
      {
        // primaryKey as jmesPath
        "primaryKey": "objectIds.id",
